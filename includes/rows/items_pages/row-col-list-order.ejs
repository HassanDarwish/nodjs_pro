<div class="col-md-9">
                            
    <div class="block">
        <div class="block-content">
            <div class="pull-left">
                <!--  <button type="button" class="btn btn-default nm"><i class="fa fa-refresh"></i></button>   -->
                <div class="btn-group">                                            
                  <!--  <button type="button" class="btn btn-default"><i class="fa fa-share"></i></button>  -->
                    <button type="button" id="submit_to_delete" class="btn btn-default"><i class="fa fa-trash-o"></i></button>
                </div>
                <!--
                    <button type="button" class="btn btn-default"><i class="fa fa-print"></i></button>
                    -->

            </div>
            <div class="pull-right">                                        
                <div class="btn-group">                                            
                    <button type="button" class="btn btn-default"><i class="fa fa-angle-left"></i></button>
                    <button type="button" class="btn btn-default"><i class="fa fa-angle-right"></i></button>
                </div>
            </div>
        </div>
        <div class="block-content np">

            <form action="delete_item" id="delete_form" method="POST">


                <input type="hidden" name="_csrf" value="<%= csrfToken %>">

            <% if (items.length > 0 ) {%>            
            <table class="table table-striped table-hover">
               
                <tr>
                    <th width="20"><input type="checkbox" class="checkall"/></th>
                    <th width="20"></th>
                    <th>Title</th>
                    <th>Description</th>
                    <th>Total amount</th>
                  <!--  <th>Date</th> -->
                </tr>
              
                <% for ( let item of items ){%>
                <tr>
                    <td><input type="checkbox" name ="id_s" value="<%=item.id%>"/></td>
                    <% if (item.total_amount>500){%>
                    <td><span class="group group-green"></span></td>
                    <%}%>
                   
                    <% if (item.total_amount>400 && item.total_amount<500){%>
                    <td><span class="group group-blue"></span></td>
                    <%}%>


                    <% if (item.total_amount<400 && item.total_amount>300 ){%>
                        <td><span class="group group-yellow"></span></td>
                        <%}%>

                        
                    <% if (item.total_amount<300  ){%>
                        <td><span class="group group-red"></span></td>
                        <%}%>
                    <td>
                    <a href="#" class="Mido" data-param1="<%=item.id%>" data-param2="<%=item.title%>" data-param3="<%=item.description%>"
                         data-toggle="modal" data-target="#modal-example-2"><strong><%=item.title%></strong></a></td>
                    
                    <td><a href="#"><strong><%=item.description%></strong></a></td>
                    <td><i class="fa fa-paperclip"><%=item.total_amount%></i></td>
                   <!-- <td>5min ago</td> -->
                </tr>
                <% } %>
              
            </table>
            <%}%>
            </form>

        </div>
        <div class="block-footer">
            <div class="pull-left"><strong>1-20</strong> of 387</div>
            <div class="pull-right">                                        
                <div class="btn-group">                                            
                    <button type="button" class="btn btn-default"><i class="fa fa-angle-left"></i></button>
                    <button type="button" class="btn btn-default"><i class="fa fa-angle-right"></i></button>
                </div>
            </div>
        </div>
    </div>
    
</div>